{
  "name": "oxidio/module-klarna",
  "replace": {
    "topconcepts/oxid-klarna-6": "self.version"
  },
  "description": "Klarna module for OXID eShop version 6.X",
  "type": "oxideshop-module",
  "extra": {
    "branch-alias": {
      "dev-oxidio-master": "5.x-dev"
    },
    "oxideshop": {
      "blacklist-filter": [
        "documentation/**/*.*"
      ],
      "target-directory": "tc/tcklarna"
    }
  },
  "require": {
    "php": ">=5.6",
    "rmccue/requests": ">=1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpunit/phpunit-selenium": ">=1.2"
  },
  "autoload": {
    "psr-4": {
      "TopConcepts\\Klarna\\": "./"
    }
  }
}